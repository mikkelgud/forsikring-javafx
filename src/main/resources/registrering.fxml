<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="354.0" prefWidth="589.0" xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.mikkelgud.person.RegistratorController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Open…" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                        <MenuItem mnemonicParsing="false" text="Revert" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Preferences…" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="169.0" prefWidth="222.0" VBox.vgrow="ALWAYS">
            <children>
                <TextField id="firstName" fx:id="firstName" layoutX="72.0" layoutY="28.0" promptText="Fornavn"/>
                <TextField id="lastName" fx:id="lastName" layoutX="72.0" layoutY="80.0" promptText="Etternavn"/>
                <TextField id="billingAddress" fx:id="billingAddress" layoutX="72.0" layoutY="133.0"
                           promptText="Fakturaadresse"/>
                <CheckBox fx:id="insuranceHouse" layoutX="277.0" layoutY="33.0" mnemonicParsing="false"
                          text="Hus og innboforsikring"/>
                <CheckBox fx:id="insuranceTravel" layoutX="276.0" layoutY="85.0" mnemonicParsing="false"
                          text="Reiseforsikring"/>
                <CheckBox fx:id="insuranceCabin" layoutX="276.0" layoutY="138.0" mnemonicParsing="false"
                          text="Fritidsboligforsikring"/>
                <CheckBox fx:id="insuranceBoat" layoutX="276.0" layoutY="186.0" mnemonicParsing="false"
                          text="Båtforsikring"/>
                <Button id="registrer" layoutX="72.0" layoutY="190.0" mnemonicParsing="false"
                        onAction="#handleRegistrerButtonAction" prefHeight="27.0" prefWidth="167.0" text="Registrer"/>
                <Label fx:id="errorLabel" layoutX="71.0" layoutY="243.0" prefHeight="17.0" prefWidth="400.0"
                       textFill="red"/>
                <Button layoutX="471.0" layoutY="260.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="64.0"
                        text="Lukk"
                        onAction="#closeRegistrationButtonHandeler" fx:id="closeRegistrationPageButton"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
