<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>
<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.mikkelgud.Controller">

    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New housing insurance" onAction="#newHouseInsurance"/>
                        <MenuItem mnemonicParsing="false" text="New boat insurance" onAction="#newBoatInsurance"/>
                        <MenuItem mnemonicParsing="false" text="Ny fritidsboligforsikring"
                                  onAction="#newCabinInsurance"/>
                        <MenuItem mnemonicParsing="false" text="New travel insurance" onAction="#newTravelInsurance"/>
                        <SeparatorMenuItem mnemonicParsing="false" text="Meld ny skade"/>
                        <MenuItem mnemonicParsing="false" text="Open file" onAction="#openFile" />
                        <MenuItem mnemonicParsing="false" text="Save" onAction="#saveFile" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <SplitPane dividerPositions="0.3151447661469933, 0.7505567928730512" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane>
                    <children>
                        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" prefWidth="-1.0" style="&#10;" text="Kundenavn" textAlignment="CENTER" wrapText="false">
                            <font>
                                <Font size="18.0" fx:id="x1" />
                            </font>
                            <textFill>
                                <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
                            </textFill>
                        </Label>
                        <TextField layoutX="11.0" layoutY="37.0" promptText="Søk på kundenavn"
                                   fx:id="customerSearchInput"/>


                        <Button layoutX="11.0" layoutY="509.0" mnemonicParsing="false"
                                onAction="#newInsuranceUserWinddowOpener" prefHeight="27.0" prefWidth="135.0"
                                text="Registrer ny kunde"/>
                  <ListView fx:id="personListView" layoutX="11.0" layoutY="72.0" prefHeight="425.0" prefWidth="254.0" />
                    </children>
                </AnchorPane>
                <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
                    <content>
                        <AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="430.0">
                            <children>
                                <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="14.0" style="&#10;" text="Kundeinformasjon" textAlignment="CENTER" textFill="$x2" wrapText="false" />
                                <ListView fx:id="currPersonListView" layoutX="14.0" layoutY="73.0" prefHeight="467.0"
                                          prefWidth="405.0"/>
                            </children>
                        </AnchorPane>
                    </content>
                </ScrollPane>
                <AnchorPane>
                    <children>
                        <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="14.0" style="&#10;" text="Kundeforsikringer" textAlignment="CENTER" textFill="$x2" wrapText="false" />
                        <ListView fx:id="currPersonInsuranceListView" layoutX="10.0" layoutY="75.0" prefHeight="465.0"
                                  prefWidth="200.0"/>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="11.0" fx:id="x3" />
                    </font>
                    <textFill>
                        <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
                    </textFill>
                </Label>
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>
    </children>
</VBox>
